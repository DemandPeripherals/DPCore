  # Clocks
NET "CK50"     IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P38"  ;    # 50.0 MHz clock
NET "CK12"     IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P39"  ;    # 12.5 MHz clock

  # Buttons
NET "BNTN1"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P13"  ;    # Button 1
NET "BNTN2"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P30"  ;    # Button 2
NET "BNTN3"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P69"  ;    # Button 3

  # LEDs
NET "LED<0>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P70"  ;    # LED 0
NET "LED<1>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P71"  ;    # LED 1
NET "LED<2>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P62"  ;    # LED 2
NET "LED<3>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P66"  ;    # LED 3
NET "LED<4>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P67"  ;    # LED 4
NET "LED<5>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P68"  ;    # LED 5
NET "LED<6>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P63"  ;    # LED 6
NET "LED<7>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P65"  ;    # LED 7

  # USB interface
NET "RXF_"      IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P89"  ;   # New data is available
NET "RD_"       IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P43"  ;   # Active low read data
NET "TXE_"      IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P88"  ;   # Transmit buffer empty (not)
NET "WR"        IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P47"  ;   # Write data on positive edge
NET "USBD<0>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P44"  ;   # USB Data 0
NET "USBD<1>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P41"  ;   # USB Data 1
NET "USBD<2>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P40"  ;   # USB Data 2
NET "USBD<3>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P36"  ;   # USB Data 3
NET "USBD<4>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P35"  ;   # USB Data 4
NET "USBD<5>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P34"  ;   # USB Data 5
NET "USBD<6>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P33"  ;   # USB Data 6
NET "USBD<7>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P32"  ;   # USB Data 7

  # 3-pin headers
NET "GPIO0"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P49"  ;    # GPIO on 3-pin header #0
NET "GPIO1"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P24"  ;    # GPIO on 3-pin header #1
NET "GPIO2"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P26"  ;    # GPIO on 3-pin header #2
NET "GPIO3"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P48"  ;    # VS2, not really accessible

  # Expansion headers
NET "JPA<0>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P92"  ;
NET "JPA<1>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P78"  ;
