NET "JPA<7>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P85"  ;

NET "JPB<0>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | DRIVE = 24 | LOC = "P86"  ;
